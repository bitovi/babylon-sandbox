<html>
  <head>
    <title>{{title}}</title>
  </head>
  <body>
    <can-import from="egospace/styles.less!" />
    <can-import from="egospace/app" export-as="viewModel" />
    <can-import from="egospace/components/demo/" />

    <h1>{{message}}</h1>

    <egospace-demo />

    {{#switch env.NODE_ENV}}
      {{#case "production"}}
        <script src="{{joinBase 'node_modules/steal/steal.production.js'}}"  main="egospace/index.stache!done-autorender"></script>
      {{/case}}
      {{#default}}
        <script src="/node_modules/steal/steal.js"></script>
      {{/default}}
    {{/switch}}
  </body>
</html>
